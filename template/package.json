{
    "name": "<%= context.basic.name %>",
    "version": "0.1.0",
    "description": "",
    <% if(context.npm.private) { %>
    "private": true,
    <% } %>
    "engines": {
        "node": ">=4.0 <7",
        "npm": ">=3.3 <4"
    },
    <% if(context.git.hasRepo) { %>
    "repository": {
        "type": "git",
        "url": "<%= context.git.gitRepo %>"
    },
    <% } %>
    "scripts": {
        "dev": "node build-scripts/dev.js",
        "prod": "node build-scripts/publish.js",
        "test": ""
    },
    "author": "<%= context.basic.author %>",
    <% if(!context.npm.private){ %>
    "license": "<%= context.npm.license %>",
    <% } %>
    "devDependencies": {
        "autoprefixer": "^6.7.6",
        "babel-cli": "^6.24.1",
        "babel-core": "^6.24.1",
        "babel-loader": "^7.1.2",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-object-assign": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-preset-env": "^1.5.2",
        "babel-preset-react": "^6.23.0",
        "bluebird": "^3.5.1",
        "chalk": "^2.3.0",
        "copy-webpack-plugin": "^4.2.0",
        "css-loader": "^0.26.2",
        "del": "^3.0.0",
        "expose-loader": "^0.7.4",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^0.10.1",
        "html-loader": "^0.4.5",
        "html-webpack-plugin": "^2.28.0",
        "less": "^2.7.3",
        "less-loader": "^2.2.3",
        "mocha": "^3.0.2",
        "node-glob": "^1.2.0",
        "ora": "^1.3.0",
        "postcss": "^5.2.18",
        "postcss-loader": "^1.3.3",
        "shelljs": "^0.7.8",
        "should": "^10.0.0",
        "stats-webpack-plugin": "^0.6.1",
        "style-loader": "^0.13.2",
        "through2": "^2.0.1",
        "url-loader": "^0.5.8",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.3",
        "webpack-merge": "^4.1.0",
        "write-file-webpack-plugin": "^3.4.2"
    },
    "dependencies": {
        "babel-polyfill": "^6.23.0",
        "react": "^15.6.2",
        "react-dom": "^15.6.2"
    }
}
